{
  "name": "api-rest-ts",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon ./src/app.ts",
    "start": "node dist/app.js",
    "start:dev": "ts-node-dev --respawn src/app.ts",
    "build": "npm run build:clean && npm run build:tsc",
    "build:clean": "rimraf dist && exit 0",
    "build:tsc": "tsc -p tsconfig.json",
    "migration:create": "npx sequelize-cli migration:create --name ",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate-rollback": "npx sequelize-cli db:migrate:undo",
    "seed:create": "npx sequelize-cli seed:generate --name ",
    "seed": "npx sequelize-cli db:seed:all",
    "seed-stg": "cross-env NODE_ENV=test npx sequelize-cli db:seed:all",
    "seed-prod": "cross-env NODE_ENV=production npx sequelize-cli db:seed:all",
    "unseed": "npx sequelize-cli db:seed:undo:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "eslint-config-prettier": "^9.1.0",
    "express": "^4.19.2",
    "joi": "^17.13.1",
    "jsonwebtoken": "^9.0.2",
    "mariadb": "^3.3.0",
    "moment-timezone": "^0.5.45",
    "mongoose": "^8.3.3",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "prettier": "^3.2.5",
    "sequelize": "^6.37.3"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.11",
    "@types/uuid": "^10.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "globals": "^15.2.0",
    "rimraf": "^5.0.5",
    "sequelize-cli": "^6.6.2",
    "ts-node-dev": "^2.0.0",
    "typescript-eslint": "^7.8.0"
  }
}
