{
  "version": "1.0.0",
  "configurations": [
    {
      "name": "Service",
      "runtimeVersion": "18.16.0",
      "preLaunchTask": "npm: build", // npm run build
      "runtimeArgs": ["start"], // npm run start
      "request": "launch",
      "runtimeExecutable": "npm",
      "type": "node",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "name": "Watch",
      "runtimeVersion": "18.16.0",
      "runtimeArgs": ["run", "start:dev"], // npm run start:dev
      "request": "launch",
      "runtimeExecutable": "npm",
      "type": "node",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
